@model List<CourseModel>
@{
    ViewData["Title"] = "Matrículas atuais";
}
<h1 class="display-4">@ViewData["Title"]:</h1>

<div class="text-center">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">IdCurso</th>
                <th scope="col">Curso</th>
                <th scope="col">IdAluno</th>
                <th scope="col">Aluno</th>
                <th scope="col"></th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null && Model.Any())
            {
                foreach (CourseModel course in Model)
                {
                    if (course.Leads.Any())
                    {
                        foreach (LeadModel lead in course.Leads)
                        {
                            <tr>
                                <th scope="row">@course.CourseId</th>
                                <td>@course.CourseName</td>
                                <td>@lead.LeadId</td>
                                <td>@lead.Name</td>
                                <td>
                                    !!!!! REMOVER ESSES BOTÕES
                                    <div class="btn-group" role="group" aria-label="Basic example">
                                        <a role="button" class="btn btn-primary">Editar</a>
                                        <a role="button" class="btn btn-danger">Apagar</a>
                                    </div>
                                </td>
                            </tr>
                        }
                    }
                }
            }
        </tbody>
    </table>
</div>

